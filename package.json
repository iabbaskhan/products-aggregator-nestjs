{
  "name": "product-aggregator-service",
  "version": "0.0.1",
  "description": "",
  "private": true,
  "scripts": {
    "build": "nest build -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "NODE_ENV=development nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "NODE_ENV=development nest start --debug --watch",
    "start:prod": "node --max-old-space-size=4096 dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prisma:generate": "ts-node scripts/prisma-with-config.ts generate",
    "prisma:push": "ts-node scripts/prisma-with-config.ts db push",
    "prisma:migrate": "ts-node scripts/prisma-with-config.ts migrate dev",
    "prisma:seed": "ts-node prisma/seed.ts",
    "prisma:studio": "ts-node scripts/prisma-with-config.ts studio",
    "prisma:reset": "ts-node scripts/prisma-with-config.ts migrate reset",
    "prisma:deploy": "ts-node scripts/prisma-with-config.ts migrate deploy",
    "db:setup": "pnpm run prisma:generate && pnpm run prisma:push && pnpm run prisma:seed"
  },
  "dependencies": {
